{
  "display_information": {
    "name": "slack-channel-message"
  },
  "features": {
    "bot_user": {
      "display_name": "slack-channel-message",
      "always_online": false
    },
    "slash_commands": [
      {
        "command": "/bot-admin",
        "url": "https://rj3nidqzuf.execute-api.us-east-1.amazonaws.com/dev/slack/events",
        "description": "Will send message to admin-channel",
        "usage_hint": "hello admin-channel",
        "should_escape": false
      }
    ]
  },
  "oauth_config": {
    "scopes": {
      "bot": [
        "channels:history",
        "channels:read",
        "chat:write",
        "chat:write.customize",
        "chat:write.public",
        "commands",
        "files:read",
        "files:write",
        "groups:history",
        "groups:read",
        "users:read"
      ]
    }
  },
  "settings": {
    "event_subscriptions": {
      "request_url": "https://rj3nidqzuf.execute-api.us-east-1.amazonaws.com/dev/slack/events",
      "bot_events": ["message.channels", "message.groups"]
    },
    "interactivity": {
      "is_enabled": true,
      "request_url": "https://rj3nidqzuf.execute-api.us-east-1.amazonaws.com/dev/slack/events"
    },
    "org_deploy_enabled": false,
    "socket_mode_enabled": false,
    "token_rotation_enabled": false
  }
}
